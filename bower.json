{
  "name": "gae-init",
  "dependencies": {
    "bootstrap": "~3",
    "bootswatch": "~3",
    "color": "2.4.1",
    "flag-icon-css": "~0",
    "font-awesome": "~4",
    "jquery": "~2",
    "moment": "~2",
    "nprogress": "https://github.com/lipis/nprogress.git#master"
  },
  "overrides": {
    "bootstrap": {
      "main": ["less/**", "fonts/*", "js/*"]
    },
    "bootswatch": {
      "main": ["**/*.less"]
    },
    "color": {
      "main": ["one-color-debug.js"]
    },
    "flag-icon-css": {
      "main": ["less/*", "flags/**"]
    },
    "font-awesome": {
      "main": ["less/*", "fonts/*"]
    },
    "moment": {
      "main": ["moment.js", "locale/*"]
    }
  }
}
